.root {
	--blue: #475b79;
	--grey: #a0a2b1;
	--ashen: #ebe1dc;
	--orange: #faac88;
	--eggplant: #986a8d;
	display: flex;
	width: 100%;
	height: 100%;
	flex-direction: column;
	background-color: rgba(0, 107, 113, 0.3);
}

.main {
	display: flex;
	height: 100%;
	align-items: center;
	justify-content: space-between;
	padding: 1em 3em;
}
